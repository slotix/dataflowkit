{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug Fetcher",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/fetch/fetch.d",
            "env":{},
            "args": [],
            "showLog": true
        },
        {
            "name": "Debug Parser",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/parse/parse.d",
            "env":{},
            "args": [],
            "showLog": true
        },
        {
            "name": "Debug Webserver",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/webserver",
            "env":{},
            "args": [],
            "showLog": true
        },
        {
            "name": "Launch test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceRoot}",
            "args": [
                "-test.run",
                "MyTestFunction"
            ]
        },
        {
            "name": "Ravi Debugger",
            "type": "lua",
            "request": "launch",
            "program": "${workspaceRoot}/main.lua",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}",
            "LUA_PATH": "${workspaceRoot}/?.lua",
            "LUA_CPATH": "${workspaceRoot}/?.dll"
        }
    ]
}